@{
    ViewBag.Title = "Home Page";
}
<div id="preloader">
    <div id="status">&nbsp;</div>
</div>
<div class="carousel fade-carousel slide" data-ride="carousel" data-interval="2000" id="bs-carousel">
    <!-- Overlay -->
    <div class="overlay"></div>

    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#bs-carousel" data-slide-to="0" class="active"></li>
        <li data-target="#bs-carousel" data-slide-to="1"></li>
        <li data-target="#bs-carousel" data-slide-to="2"></li>
        <li data-target="#bs-carousel" data-slide-to="3"></li>
        <li data-target="#bs-carousel" data-slide-to="4"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item slides active">
            <div class="slide-1"></div>
            <div class="hero">
                <hgroup>
                    <h1><span>@Resources.Resources.creative</span></h1>
                    <h3>@Resources.Resources.awsomeProject</h3>
                </hgroup>
                @Html.ActionLink(" " + Resources.Resources.searchmyFuture, "SearchFuture", "SearchFuture", null, new { @class = "btn btn-hero btn-lg" })
            </div>
        </div>
        <div class="item slides">
            <div class="slide-2"></div>
            <div class="hero">
                <hgroup>
                    <h1>You are <span>smart</span></h1>
                    <h3>Plan your future</h3>
                </hgroup>
                @Html.ActionLink(" " + Resources.Resources.lstPrograms, "Index", "Programs", null, new { @class = "btn btn-hero btn-lg" })
            </div>
        </div>
        <div class="item slides">
            <div class="slide-3"></div>
            <div class="hero">
                <hgroup>
                    <h1>You are <span>amazing</span></h1>
                    <h3>Share your study experience with us</h3>
                </hgroup>
                <a class="btn btn-hero btn-lg" role="button" href="~/Faculties/Index">Comment on your study</a>
            </div>
        </div>
        <div class="item slides">
            <div class="slide-4"></div>
            <div class="hero">
                <hgroup>
                    <h1>You are <span>amazing</span></h1>
                    <h3>Share your study experience with us</h3>
                </hgroup>
                <a class="btn btn-hero btn-lg" role="button" href="~/Faculties/Index">Comment on your study</a>
            </div>
        </div>
        <div class="item slides">
            <div class="slide-5"></div>
            <div class="hero">
                <hgroup>
                    <h1>You are <span>amazing</span></h1>
                    <h3>Share your study experience with us</h3>
                </hgroup>
                <a class="btn btn-hero btn-lg" role="button" href="~/Faculties/Index">Comment on your study</a>
            </div>
        </div>

    </div>
</div>
